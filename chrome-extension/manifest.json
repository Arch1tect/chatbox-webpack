{
  "name": "Chat Anywhere webpack",
  "description": "Chat with people who are browsing the same web page!",
  "version": "1.0.7",
  "permissions": ["activeTab", "storage", "unlimitedStorage"],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["chatbox-only/content.js","chatbox-only/danmu.js"],
    "css": ["chatbox-only/content.css"]
  }],
  "icons": { 
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon.png" 
  },
  "browser_action": {
      "default_title": "Chat Anywhere",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "web_accessible_resources": ["chatbox-only/chatbox-frame.html"]

}
